@{
    ViewData["Title"] = "Create Part";
}

<script src="~/js/addpart.js" type="text/javascript"></script>


<div class="container" style="border-style: solid;">
    <div class="text-center">
        <h1 class="display-4">
            Aircraft Id: @(ViewContext.RouteData.Values["Id"])
        </h1>

        <div class="row">
            <div class="col-1">
                <label>Part Name:</label>
            </div>

            <div class="col-2">
                <button type="submit" class="btn btn-primary" onclick="productAddToTable()">Add</button>
            </div>
        </div>


    </div>
</div>


<input type="hidden" value='@ViewContext.RouteData.Values["id"]' id="routeDataId" />